{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ocean Breeze Light",
  "displayName": "Ocean Breeze Light",
  "description": "A light theme with ocean-inspired colors featuring teal, warm orange, and soft purple accents",
  "author": "Your Name",
  "type": "light",
  "semanticHighlighting": true,
  
  "semanticTokenColors": {
    // Enhanced semantic token support for modern languages
    "enumMember": {
      "foreground": "#0fa3b1"
    },
    "variable.constant": {
      "foreground": "#0197f6"
    },
    "variable.defaultLibrary": {
      "foreground": "#f7a072"
    },
    "class": {
      "foreground": "#f7a072"
    },
    "interface": {
      "foreground": "#0fa3b1"
    },
    "namespace": {
      "foreground": "#ca7bf7"
    },
    "type": {
      "foreground": "#f7a072"
    },
    "parameter": {
      "foreground": "#170f11"
    },
    "property": {
      "foreground": "#f7a072"
    },
    "method": {
      "foreground": "#ca7bf7"
    },
    "function": {
      "foreground": "#ca7bf7"
    },
    "keyword": {
      "foreground": "#0fa3b1"
    },
    "decorator": {
      "foreground": "#ca7bf7"
    },
    "macro": {
      "foreground": "#f7a072"
    }
  },
  
  "tokenColors": [
    // Base language elements
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#170f11a5"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0fa3b1"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.logical",
        "keyword.operator.bitwise",
        "keyword.operator.channel"
      ],
      "settings": {
        "foreground": "#0fa3b1"
      }
    },
    {
      "name": "Storage and Types",
      "scope": [
        "storage",
        "storage.type",
        "support.type",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#ca7bf7"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    {
      "name": "Variables and Properties",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.parameter",
        "meta.object-literal.key",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    {
      "name": "Language Variables",
      "scope": [
        "variable.language",
        "variable.parameter.function.language.special"
      ],
      "settings": {
        "foreground": "#f7a072",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#0197f6"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#0fa3b1"
      }
    },
    {
      "name": "String Punctuation",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#0fa3b1"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp",
        "constant.other.character-class.regexp",
        "keyword.operator.quantifier.regexp"
      ],
      "settings": {
        "foreground": "#0fa3b1"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#0fa3b1"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition"
      ],
      "settings": {
        "foreground": "#170f11"
      }
    },
    
    // Language-specific enhancements
    {
      "name": "JavaScript/TypeScript Import/Export",
      "scope": [
        "keyword.operator.expression.import",
        "keyword.operator.expression.export",
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#ca7bf7"
      }
    },
    {
      "name": "TypeScript/JavaScript Decorators",
      "scope": [
        "meta.decorator",
        "meta.decorator.ts",
        "meta.decorator.tsx",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#ca7bf7"
      }
    },
    {
      "name": "JSX/TSX Tags",
      "scope": [
        "entity.name.tag.jsx",
        "entity.name.tag.tsx",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    {
      "name": "Python Magic Methods",
      "scope": [
        "support.variable.magic.python"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    {
      "name": "Python Function Decorators",
      "scope": [
        "meta.function.decorator.python",
        "support.token.decorator.python"
      ],
      "settings": {
        "foreground": "#ca7bf7"
      }
    },
    {
      "name": "Rust Lifetime Annotations",
      "scope": [
        "storage.modifier.lifetime.rust",
        "entity.name.lifetime.rust"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    
    // Markup and Documentation
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#f7a072",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold.markdown"
      ],
      "settings": {
        "foreground": "#0197f6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#0fa3b1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#0fa3b1"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#ca7bf7"
      }
    },
    
    // HTML/XML
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0197f6"
      }
    },
    {
      "name": "HTML Attribute IDs",
      "scope": [
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#ca7bf7"
      }
    },
    
    // CSS/SCSS/LESS
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss"
      ],
      "settings": {
        "foreground": "#0fa3b1"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.property-value.scss"
      ],
      "settings": {
        "foreground": "#0197f6"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "meta.selector.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    
    // JSON
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json",
        "string.quoted.json"
      ],
      "settings": {
        "foreground": "#f7a072"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "string.quoted.json",
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#0fa3b1"
      }
    },
    
    // Error states
    {
      "name": "Invalid/Illegal",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ca4b51"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#e9a700",
        "fontStyle": "strikethrough"
      }
    }
  ],
  
